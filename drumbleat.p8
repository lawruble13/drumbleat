-- lowrez misc functions
function tile_bg(n)
	n = n or 8
	for i, v in ipairs(bg.tiles) do
		if i > n then
			bg.tiles[i] = bg.tiles[i-n]
		end
	end
	for i=1,n do
		local r = flr(rnd(10))
		if r<4 then
			bg.tiles[i] = r
		else
			bg.tiles[i] = -1
		end
	end
	if record_tiles then
		local l = #(bg.tiles)
		for i = 1,l/8 do
			csv_print_file("bg_tiles.csv","",{unpack(bg.tiles,l-8*i+1,l-8*i+8)},i==1)
		end
	end
end

function test_platforms()
	gw.platforms[2]={
		x=12,
		y=10,
		m=0.6,
		l=20
	}
	gw.platforms[3]={
		x=0,
		y=30,
		m=-0.4,
		l=14
	}
	gw.platforms[4]={
		x=20,
		y=64,
		m=0.1,
		l=23
	}
	gw.platforms[5]={
		x=28,
		y=30,
		m=1,
		l=30
	}
end
